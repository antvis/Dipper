{
  "private": true,
  "name": "dipper",
  "version": "1.0.0",
  "scripts": {
    "clean": "lerna run clean",
    "start": "dumi dev",
    "docs:build": "dumi build",
    "version": "lerna version --force-publish --conventional-commits --exact --no-changelog",
    "version:prerelease": "lerna version --force-publish --conventional-prerelease",
    "prerelease": "yarn build",
    "release": "lerna publish from-package --registry https://registry.npmjs.org/ --force-publish && lerna run sync",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "yarn clean && lerna run build",
    "build-watch": "yarn clean && lerna run watch",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "sync": "lerna run sync",
    "test:coverage": "umi-test --coverage",
    "link": "lerna exec -- yarn link",
    "unlink": "lerna exec -- yarn unlink"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged && yarn run build"
  },
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "antd": "^4.16.13",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.18.24",
    "@antv/g2plot": "^2.3.39",
    "@antv/l7": "^2.7.30",
    "@antv/l7-draw": "^2.4.20",
    "@antv/l7-react": "^2.3.9",
    "@turf/turf": "^6.5.0",
    "classnames": "^2.3.1",
    "dumi-theme-mobile": "^1.1.21",
    "inversify-react": "^1.0.1",
    "prettier": "^2.5.1"
  },
  "devDependencies": {
    "@ant-design/pro-card": "^1.18.24",
    "@turf/turf": "^6.5.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.178",
    "@types/react": "^16.14.2",
    "@umijs/fabric": "^2.10.1",
    "@umijs/plugin-analytics": "^0.2.2",
    "@umijs/test": "^3.0.5",
    "lodash": "^4.17.21",
    "ahooks": "^3.1.5",
    "antd": "^4.18.3",
    "antd-mobile": "^5.1.0",
    "antd-mobile-icons": "^0.2.2",
    "dumi": "^1.0.17",
    "dumi-theme-mobile": "^1.1.21",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "jest": "^27.3.1",
    "lerna": "^4.0.0",
    "lerna-ci": "^0.0.8",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.22.12",
    "yorkie": "^2.0.0"
  }
}
